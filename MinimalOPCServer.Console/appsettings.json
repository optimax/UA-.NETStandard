{
  "ApplicationName": "MinimalOpcServer",
  "ApplicationType": "Server_0",
  "ApplicationUri": "urn:localhost:UA:MinimalOpcServer",

  "SecurityConfiguration": {
    "ApplicationCertificate": {
      "StoreType": "Directory",
      "StorePath": "OPC Foundation\\CertificateStores\\MachineDefault",
      "SubjectName": "CN=Minimal Data Access Server, C=US, S=Wisconsin, O=Optimax Corporation, DC=localhost"
    },

    "TrustedIssuerCertificates": {
      "StoreType": "Directory",
      "StorePath": "OPC Foundation\\CertificateStores\\UA Certificate Authorities"
    },
    "TrustedPeerCertificates": {
      "StoreType": "Directory",
      "StorePath": "OPC Foundation\\CertificateStores\\UA Applications"
    },


    "RejectedCertificateStore": {
      "StoreType": "Directory",
      "StorePath": "OPC Foundation\\CertificateStores\\RejectedCertificates"
    },

    "MinimumCertificateKeySize": 2048,
    "AddAppCertToTrustedStore": true
  },
  "TransportConfigurations": [],
  "TransportQuotas": {
    "OperationTimeout": 600000,
    "MaxStringLength": 1048576,
    "MaxByteStringLength": 1048576,
    "MaxArrayLength": 65535,
    "MaxMessageSize": 4194304,
    "MaxBufferSize": 65535,
    "ChannelLifetime": 300000,
    "SecurityTokenLifetime": 3600000
  },
  "ServerConfiguration": {
    "BaseAddresses": [
      "https://omopc1509:62545/MinimalOPCServer/",
      "opc.tcp://omopc1509:62546/MinimalOPCServer"
    ],
    "AlternateBaseAddresses": [],
    "SecurityPolicies": [
      {
        "SecurityMode": 1,
        "SecurityPolicyUri": "http://opcfoundation.org/UA/SecurityPolicy#None"
      },
      {
        "SecurityMode": 2,
        "SecurityPolicyUri": "http://opcfoundation.org/UA/SecurityPolicy#Basic256Sha256"
      },
      {
        "SecurityMode": 3,
        "SecurityPolicyUri": "http://opcfoundation.org/UA/SecurityPolicy#Basic256Sha256"
      }
    ],
    "UserTokenPolicies": [
      {
        "PolicyId": null,
        "TokenType": 0,
        "IssuedTokenType": null,
        "IssuerEndpointUrl": null,
        "SecurityPolicyUri": null
      },
      {
        "PolicyId": null,
        "TokenType": 1,
        "IssuedTokenType": null,
        "IssuerEndpointUrl": null,
        "SecurityPolicyUri": null
      },
      {
        "PolicyId": null,
        "TokenType": 2,
        "IssuedTokenType": null,
        "IssuerEndpointUrl": null,
        "SecurityPolicyUri": null
      },
      {
        "PolicyId": null,
        "TokenType": 3,
        "IssuedTokenType": "urn:oasis:names:tc:SAML:1.0:assertion:Assertion",
        "IssuerEndpointUrl": null,
        "SecurityPolicyUri": null
      }
    ],
    "MinRequestThreadCount": 10,
    "DiagnosticsEnabled": false,
    "MaxRequestThreadCount": 100,
    "MaxSessionCount": 100,
    "MaxQueuedRequestCount": 200,
    "MinSessionTimeout": 10000,
    "MaxSessionTimeout": 3600000,
    "MaxBrowseContinuationPoints": 10,
    "MaxQueryContinuationPoints": 10,
    "MaxHistoryContinuationPoints": 100,
    "MaxRequestAge": 600000,
    "MinPublishingInterval": 100,
    "MaxPublishingInterval": 3600000,
    "PublishingResolution": 50,
    "MaxSubscriptionLifetime": 3600000,
    "MaxMessageQueueSize": 10,
    "MaxNotificationQueueSize": 100,
    "MaxNotificationsPerPublish": 1000,
    "MinMetadataSamplingInterval": 1000,
    "AvailableSamplingRates": [
      {
        "Start": 5.0,
        "Increment": 5.0,
        "Count": 20
      },
      {
        "Start": 100.0,
        "Increment": 100.0,
        "Count": 4
      },
      {
        "Start": 500.0,
        "Increment": 250.0,
        "Count": 2
      },
      {
        "Start": 1000.0,
        "Increment": 500.0,
        "Count": 20
      }
    ],
    "MaxRegistrationInterval": 30000,
    "NodeManagerSaveFile": "Orchestrator.nodes.xml",
    "MinSubscriptionLifetime": 10000,
    "MaxPublishRequestCount": 20,
    "MaxSubscriptionCount": 100,
    "MaxEventQueueSize": 10000,
    "ServerProfileArray": [
      "Standard UA Server Profile"
    ],
    "ShutdownDelay": 5,
    "ServerCapabilities": [
      "DA"
    ],
    "SupportedPrivateKeyFormats": [],
    "MaxTrustListSize": 0,
    "MultiCastDnsEnabled": false
